@model IEnumerable<University>

<h1>Universities' List</h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>Id</th>
			<th>Name</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var university in Model)
		{
			<tr>
				<td>@university.Id</td>
				<td>@university.Name</td>
			</tr>
		}
	</tbody>
</table>

